<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.travelmate.hibernate.Admin" table="admin">
<id column="ID" name="id" type="integer" />
<property column="AdminName" name="AdminName" type="string" />
<property column="AdminEmail" name="AdminEmail" type="string" />
<property column="AdminUserName" name="AdminUserName" type="string" />
<property column="AdminPassword" name="AdminPassword" type="string" />

</class>
</hibernate-mapping>