<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="com.travelmate.hibernate.TravelInventory" table="travelinventory">
<id column="ID" name="Id" type="integer" />
<property column="TravelName" name="TravelName"/>
<property column="UserName" name="UserName"/>
<property column="ProductCategory" name="ProductCategory"/>
<property column="ProductName" name="ProductName"/>
<property column="ProductQuantity" name="ProductQuantity" />
<property column="ProductPic" name="ProductPic" />
</class>
</hibernate-mapping>

